start;
	repeat 4;
		move 60;
		rotateclockwise 90;
		repeat 4;
			rotateclockwise 90;
			move 50;
			repeat 4;
				move 40;
				rotateclockwise 90;
				repeat 4;
					move 30;
					rotatecounterclockwise 90;
					repeat 4;
						move 20;
						rotateclockwise 90;
					end;
				end;
			end;
		end;
	end;
end;
